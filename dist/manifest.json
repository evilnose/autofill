{
  "manifest_version": 2,
  "name": "College App Autofill",
  "version": "1.0.0",
  "description": "Fill in college application forms at the press of a button.",
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "activeTab"
  ],
  "sandbox": {
    "pages": [
      "index.html"
    ]
  }
}
