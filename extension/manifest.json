{
  "manifest_version": 2,
  "name": "College App Autofill",
  "version": "1.0.0",
  "description": "Fill in college application forms at the press of a button.",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "common/jquery.min.js", "preprocess/moment.min.js",
      "preprocess/constants.js", "preprocess/ext_helpers.js",
      "preprocess/info_retrieval.js", "background/background.js",
      "preprocess/parser.js", "preprocess/process_flow.js"
    ]
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "activeTab",
    "storage"
  ]
}
