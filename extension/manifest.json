{
  "manifest_version": 2,
  "name": "College App Autofill",
  "version": "1.0.0",
  "description": "Fill in college application forms at the press of a button.",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/jquery.min.js", "js/eventPage.js"]
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "activeTab",
    "storage"
  ]
}
