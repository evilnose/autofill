{
  "manifest_version": 2,
  "name": "College App Autofill",
  "version": "1.0.0",
  "description": "Fill in college application forms at the press of a button.",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery.min.js", "content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
